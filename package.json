{
  "name": "dev-cookie-sync",
  "displayName": "Dev Cookie Sync",
  "version": "0.0.1",
  "description": "将线上 Cookie 同步到本地",
  "author": "yx1aoq1",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "antd": "^5.27.0",
    "plasmo": "0.90.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "permissions": [
      "cookies",
      "storage"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ],
    "background": {
      "service_worker": "background.ts"
    }
  }
}
